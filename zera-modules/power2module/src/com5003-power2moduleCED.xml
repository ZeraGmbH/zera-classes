<?xml version="1.0"?>
<pow2modconfpar>
    <configuration>
        <connectivity>
            <debuglevel>0</debuglevel>
            <ethernet>
                <resourcemanager>
                    <ip>127.0.0.1</ip>
                    <port>6312</port>
                </resourcemanager>
                <pcbserver>
                    <ip>127.0.0.1</ip>
                    <port>6307</port>
                </pcbserver>
                <dspserver>
                    <ip>127.0.0.1</ip>
                    <port>6310</port>
                </dspserver>
            </ethernet>
        </connectivity>
        <measure>
            <modes>
                <n>1</n>
                <m1>4LW</m1>
            </modes>
            <system>
                <pms1>m0,m3</pms1>
                <pms2>m1,m4</pms2>
                <pms3>m2,m5</pms3>
            </system>
            <integrationmode>period</integrationmode>
            <movingwindow>
                <on>0</on>
                <time>0.5</time>
            </movingwindow>
        </measure>
        <frequencyoutput>
            <frequency>200000</frequency>
            <frequencyact>signalperiod</frequencyact>
            <output>
                <n>2</n>
                <fout1>
                    <name>fo1</name>
                    <source>pmss</source>
                    <type>+</type>
                </fout1>
                <fout2>
                    <name>fo2</name>
                    <source>pmss</source>
                    <type>-</type>
                </fout2>
            </output>
        </frequencyoutput>
    </configuration>
    <parameter>
        <measuringmode>4LW</measuringmode>
        <interval>
            <time>10</time>
            <period>200</period>
        </interval>
    </parameter>

</pow2modconfpar>
