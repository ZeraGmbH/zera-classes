<?xml version="1.0" encoding="ISO-8859-1"?>

<rangemodconfpar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <configuration>
        <connectivity>
            <debuglevel>0</debuglevel>
            <ethernet>
                <resourcemanager>
                    <ip>192.168.7.222</ip>
                    <port>6312</port>
                </resourcemanager>
                <pcbserver>
                    <ip>192.168.7.222</ip>
                    <port>6307</port>
                </pcbserver>
                <dspserver>
                    <ip>192.168.7.222</ip>
                    <port>6310</port>
                </dspserver>
            </ethernet>
        </connectivity>
        <sense>
            <withgrouping>1</withgrouping>
            <withrangeauto>1</withrangeauto>
            <withoverload>1</withoverload>
            <channel>
                <n>6</n>
                <ch1>m0</ch1>
                <ch2>m1</ch2>
                <ch3>m2</ch3>
                <ch4>m3</ch4>
                <ch5>m4</ch5>
                <ch6>m5</ch6>
            </channel>
            <subdc>
                <n>4</n>
                <ch1>m2</ch1>
                <ch2>m3</ch2>
                <ch3>m4</ch3>
                <ch4>m5</ch4>
            </subdc>
            <group>
                <n>3</n>
                <gr1>
                    <n>3</n>
                    <ch1>UL1</ch1>
                    <ch2>UL2</ch2>
                    <ch3>UL3</ch3>
                </gr1>
                <gr2>
                    <n>3</n>
                    <ch1>IL1</ch1>
                    <ch2>IL2</ch2>
                    <ch3>IL3</ch3>
                </gr2>
                <gr3>
                    <n>6</n>
                    <ch1>REF1</ch1>
                    <ch2>REF2</ch2>
                    <ch3>REF3</ch3>
                    <ch4>REF4</ch4>
                    <ch5>REF5</ch5>
                    <ch6>REF6</ch6>
                </gr3>
            </group>
        </sense>
        <measure>
            <interval>0.4</interval>
        </measure>
        <adjustment>
             <interval>0.5</interval>
        </adjustment>
    </configuration>

    <parameter>
        <sense>
            <m0>
                <range>60V</range>
            </m0>
            <m1>
                <range>60V</range>
            </m1>
            <m2>
                <range>60V</range>
            </m2>
            <m3>
                <range>1.0A</range>
            </m3>
            <m4>
                <range>1.0A</range>
            </m4>
            <m5>
                <range>1.0A</range>
            </m5>
            <grouping>0</grouping>
            <rangeauto>0</rangeauto>
        </sense>
    </parameter>

</rangemodconfpar> 
