<?xml version="1.0"?>
<sec1modconfpar>
    <configuration>
        <connectivity>
            <debuglevel>0</debuglevel>
            <ethernet>
                <resourcemanager>
                    <ip>127.0.0.1</ip>
                    <port>6312</port>
                </resourcemanager>
                <pcbserver>
                    <ip>127.0.0.1</ip>
                    <port>6307</port>
                </pcbserver>
                <secserver>
                    <ip>127.0.0.1</ip>
                    <port>6305</port>
                </secserver>
            </ethernet>
        </connectivity>
        <measure>
            <modes>
                <n>2</n>
                <mod1>mrate</mod1>
                <mod2>energy</mod2>
            </modes>
            <embedded>1</embedded>
            <dutinput>
                <n>2</n>
                <inp1>sh0</inp1>
                <inp2>fi0</inp2>
            </dutinput>
            <refinput>
                <n>1</n>
                <inp1>fo0</inp1>
            </refinput>
            <interval>2.0</interval>
        </measure>
    </configuration>
    <parameter>
        <measure>
            <dutinput>fi0</dutinput>
            <refinput>fo0</refinput>
            <mode>energy</mode>
            <dutconstant>5e+08</dutconstant>
            <refconstant>5e+08</refconstant>
            <target>500000</target>
            <energy>0.001</energy>
            <mrate>500000</mrate>
        </measure>
    </parameter>
</sec1modconfpar>
