<?xml version="1.0" encoding="ISO-8859-1"?>

<sec1modconfpar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <configuration>
        <connectivity>
            <debuglevel>0</debuglevel>
            <ethernet>
                <resourcemanager>
                    <ip>192.168.7.222</ip>
                    <port>6312</port>
                </resourcemanager>
                <pcbserver>
                    <ip>192.168.7.222</ip>
                    <port>6307</port>
                </pcbserver>
                <secserver>
                    <ip>192.168.7.222</ip>
                    <port>6305</port>
                </secserver>
            </ethernet>
        </connectivity>
        <measure>
            <dutinput>
                <n>2</n>
                <inp1>
                    <input>sh0</input>
                    <muxer>1</muxer>
                </inp1>
                <inp2>
                    <input>fi0</input>
                    <muxer>2</muxer>
                </inp2>
            </dutinput>
            <refinput>
                <n>1</n>
                <inp1>
                    <input>fo0</input>
                    <muxer>1</muxer>
                </inp1>
            </refinput>
            <interval>2.0</interval>
        </measure>
    </configuration>

    <parameter>
        <measure>
            <dutinput>sh0</dutinput>
            <refinput>fo0</refinput>
            <mode>tcount</mode>
            <dutconstant>1000.0</dutconstant>
            <refconstant>1000.0</refconstant>
            <target>12000000</target>
            <energy>1.0</energy>
            <mrate>10</mrate>
        </measure>
    </parameter>

</sec1modconfpar>
