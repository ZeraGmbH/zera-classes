<?xml version="1.0"?>
<sec1modconfpar>
    <configuration>
        <connectivity>
            <ethernet>
                <resourcemanager>
                    <ip>127.0.0.1</ip>
                    <port>6312</port>
                </resourcemanager>
                <pcbserver>
                    <ip>127.0.0.1</ip>
                    <port>6307</port>
                </pcbserver>
                <secserver>
                    <ip>127.0.0.1</ip>
                    <port>6305</port>
                </secserver>
            </ethernet>
        </connectivity>
        <measure>
            <dutinput>
                <n>4</n>
                <inp1>fi0</inp1>
                <inp2>fi1</inp2>
                <inp3>fi2</inp3>
                <inp4>fi3</inp4>
            </dutinput>
            <refinput>
                <n>3</n>
                <inp1>fo0</inp1>
                <inp2>fo1</inp2>
                <inp3>fo2</inp3>
            </refinput>
        </measure>
    </configuration>
    <parameter>
        <measure>
            <dutinput>fi0</dutinput>
            <refinput>fo0</refinput>
            <mode>energy</mode>
            <dutconstant>10000</dutconstant>
            <dutconstantunit>none</dutconstantunit>
            <refconstant>5e+08</refconstant>
            <target>500000</target>
            <energy>0.001</energy>
            <mrate>10</mrate>
            <continous>0</continous>
            <upperlimit>10.0</upperlimit>
            <lowerlimit>-10.0</lowerlimit>
            <resultunit>%</resultunit>
        </measure>
    </parameter>
</sec1modconfpar>
