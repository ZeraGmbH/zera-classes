<?xml version="1.0" encoding="ISO-8859-1"?>

<pow1modconfpar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <configuration>
        <measure>
            <modes>
                <n>3</n>
                <m1>2LW</m1>
                <m2>2LB</m2>
                <m3>2LS</m3>
            </modes>
            <system>
                <pms1>m0,m3</pms1>
                <pms2>m1,m4</pms2>
                <pms3>m2,m5</pms3>
            </system>
            <integrationmode>time</integrationmode>
            <movingwindow>
                <on>1</on>
                <time>0.5</time>
            </movingwindow>
        </measure>
        <frequencyoutput>
            <frequency>200000</frequency>
            <frequencydefault>200000</frequencydefault>
            <frequencyact>signalperiod</frequencyact>
            <output>
                <n>1</n>
                <fout1>
                    <name>fo3</name>
                    <source>pmss</source>
                    <type>+-</type>
                    <name_displayed>fOUT4</name_displayed>
                    <scaling>
                        <uscale>
                            <inputentity>1020</inputentity>
                            <inputcomponent>INF_PreScalingInfoGroup0</inputcomponent>
                        </uscale>
                        <iscale>
                            <inputentity>1020 </inputentity>
                            <inputcomponent>INF_PreScalingInfoGroup1</inputcomponent>
                        </iscale>
                    </scaling>
                </fout1>
            </output>
        </frequencyoutput>
        <enableSCPICommands>1</enableSCPICommands>
    </configuration>
    <parameter>
        <measuringmode>2LW</measuringmode>
        <modePhases>
            <n>3</n>
            <m1>2LW,001</m1>
            <m2>2LB,001</m2>
            <m3>2LS,001</m3>
        </modePhases>
        <interval>
            <time>1</time>
            <period>200</period>
        </interval>
    </parameter>
</pow1modconfpar>
