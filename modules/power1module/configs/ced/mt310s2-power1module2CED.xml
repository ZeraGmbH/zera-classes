<?xml version="1.0" encoding="ISO-8859-1"?>

<pow1modconfpar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <configuration>
        <measure>
            <modes>
                <n>4</n>
                <m1>4LB</m1>
                <m2>4LBK</m2>
                <m3>3LB</m3>
                <m4>2LB</m4>
            </modes>
            <system>
                <pms1>m0,m3</pms1>
                <pms2>m1,m4</pms2>
                <pms3>m2,m5</pms3>
            </system>
            <integrationmode>period</integrationmode>
            <movingwindow>
                <on>0</on>
                <time>0.5</time>
            </movingwindow>
        </measure>
        <frequencyoutput>
            <frequency>200000</frequency>
            <frequencyact>signalperiod</frequencyact>
            <output>
                <n>0</n>
            </output>
        </frequencyoutput>
        <enableSCPICommands>1</enableSCPICommands>
    </configuration>
    <parameter>
        <measuringmode>4LB</measuringmode>
        <modePhases>
            <n>1</n>
            <m1>2LB,100</m1>
        </modePhases>
        <interval>
            <time>4.0</time>
            <period>200</period>
        </interval>
    </parameter>
</pow1modconfpar>
