<?xml version="1.0"?>
<pow1modconfpar>
    <configuration>
        <connectivity>
            <ethernet>
                <resourcemanager>
                    <ip>127.0.0.1</ip>
                    <port>6312</port>
                </resourcemanager>
                <pcbserver>
                    <ip>127.0.0.1</ip>
                    <port>6307</port>
                </pcbserver>
                <dspserver>
                    <ip>127.0.0.1</ip>
                    <port>6310</port>
                </dspserver>
            </ethernet>
        </connectivity>
        <measure>
            <modes>
                <n>3</n>
                <m1>4LW</m1>
                <m2>3LW</m2>
                <m3>2LW</m3>
            </modes>
            <system>
                <pms1>m0,m3</pms1>
                <pms2>m1,m4</pms2>
                <pms3>m2,m5</pms3>
            </system>
            <integrationmode>time</integrationmode>
            <movingwindow>
                <on>1</on>
                <time>0.5</time>
            </movingwindow>
        </measure>
        <frequencyoutput>
            <frequency>200000</frequency>
            <frequencyact>signalperiod</frequencyact>
            <output>
            <n>1</n>
            <fout1>
                <name>fo1</name>
                <source>pmss</source>
                <type>+-</type>
                <scaling>
                    <uscale>
                        <inputentity>1020</inputentity>
                        <inputcomponent>INF_PreScalingInfoGroup0</inputcomponent>
                    </uscale>
                    <iscale>
                        <inputentity>1020 </inputentity>
                        <inputcomponent>INF_PreScalingInfoGroup1</inputcomponent>
                    </iscale>
                </scaling>
            </fout1>
            </output>
        </frequencyoutput>
        <enableSCPICommands>1</enableSCPICommands>
    </configuration>
    <parameter>
        <measuringmode>4LW</measuringmode>
        <modePhases>
            <n>1</n>
            <m1>2LW,100</m1>
        </modePhases>
        <interval>
            <time>1</time>
            <period>200</period>
        </interval>
    </parameter>
</pow1modconfpar>
