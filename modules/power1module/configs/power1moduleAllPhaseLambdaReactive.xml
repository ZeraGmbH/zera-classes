<?xml version="1.0" encoding="ISO-8859-1"?>

<pow1modconfpar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <configuration>
        <measure>
            <modes>
                <n>1</n>
                <m1>4LB</m1>
            </modes>
            <system>
                <pms1>m0,m3</pms1>
                <pms2>m1,m4</pms2>
                <pms3>m2,m5</pms3>
            </system>
            <disablephaseselect>1</disablephaseselect>
            <integrationmode>time</integrationmode>
            <movingwindow>
                <on>1</on>
                <time>0.5</time>
            </movingwindow>
        </measure>
        <frequencyoutput>
            <frequency>200000</frequency>
            <frequencydefault>200000</frequencydefault>
            <frequencyact>signalperiod</frequencyact>
            <output>
                <n>0</n>
            </output>
        </frequencyoutput>
        <enableSCPICommands>0</enableSCPICommands>
    </configuration>
    <parameter>
        <measuringmode>4LB</measuringmode>
        <modePhases>
            <n>0</n>
        </modePhases>
        <interval>
            <time>1</time>
            <period>200</period>
        </interval>
    </parameter>
</pow1modconfpar>
