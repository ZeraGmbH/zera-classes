<?xml version="1.0" encoding="ISO-8859-1"?>

<pow1modconfpar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <configuration>
        <connectivity>
            <ethernet>
                <resourcemanager>
                    <ip>127.0.0.1</ip>
                    <port>6312</port>
                </resourcemanager>
                <pcbserver>
                    <ip>127.0.0.1</ip>
                    <port>6307</port>
                </pcbserver>
                <dspserver>
                    <ip>127.0.0.1</ip>
                    <port>6310</port>
                </dspserver>
            </ethernet>
        </connectivity>
        <measure>
            <modes>
                <n>4</n>
                <m1>4LS</m1>
                <m2>4LSg</m2>
                <m3>2LS</m3>
                <m4>2LSg</m4>
            </modes>
            <system>
                <pms1>m0,m3</pms1>
                <pms2>m1,m4</pms2>
                <pms3>m2,m5</pms3>
            </system>
            <integrationmode>time</integrationmode>
            <movingwindow>
                <on>1</on>
                <time>0.5</time>
            </movingwindow>
        </measure>
        <frequencyoutput>
            <frequency>200000</frequency>
            <frequencyact>signalperiod</frequencyact>
            <output>
                <n>1</n>
                <fout1>
                    <name>fo2</name>
                    <source>pmss</source>
                    <type>+-</type>
                    <name_displayed>fOUT3</name_displayed>
                    <scaling>
                        <uscale>
                            <inputentity>1020</inputentity>
                            <inputcomponent>INF_PreScalingInfoGroup0</inputcomponent>
                        </uscale>
                        <iscale>
                            <inputentity>1020 </inputentity>
                            <inputcomponent>INF_PreScalingInfoGroup1</inputcomponent>
                        </iscale>
                    </scaling>
                </fout1>
            </output>
        </frequencyoutput>
        <enableSCPICommands>1</enableSCPICommands>
    </configuration>
    <parameter>
        <measuringmode>4LS</measuringmode>
        <modePhases>
            <n>2</n>
            <m1>2LS,001</m1>
            <m2>2LSg,001</m2>
        </modePhases>
        <interval>
            <time>1</time>
            <period>200</period>
        </interval>
    </parameter>
</pow1modconfpar>
